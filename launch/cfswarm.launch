      <arg name="color_prop_back" default="$(arg color_prop_back_2)" />
      <arg name="x" default="$(arg x_2)"/>
      <arg name="y" default="$(arg y_2)"/>
      <arg name="z" default="$(arg z_2)"/>
    </include>
  </group>

   <group ns="$(arg cfPrefix)3">
